{
  "extends": ["react-app", "react-app/jest"],
  "rules": {
    "no-unused-vars": "warn",
    "react-hooks/exhaustive-deps": "warn",
    "no-useless-escape": "warn",
    "no-loop-func": "warn",
    "no-control-regex": "warn",
    "default-case": "warn",
    "eqeqeq": "warn",
    "no-mixed-operators": "warn"
  }
}
